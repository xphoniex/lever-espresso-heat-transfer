FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/piston";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    piston_to_surronding_air
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    piston_to_water
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
}


