FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/piston";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    piston_to_surronding_air
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    piston_to_water
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
}


