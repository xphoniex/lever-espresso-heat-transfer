FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/water";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    water_to_piston
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    water_to_surronding_air
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    minX
    {
        type            zeroGradient;
        value           uniform 0;
    }
    maxX
    {
        type            fixedValue;
        value           uniform 0;
    }
}


