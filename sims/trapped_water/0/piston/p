FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/piston";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    piston_to_surronding_air
    {
        type            calculated;
        value           uniform 0;
    }
    piston_to_water
    {
        type            calculated;
        value           uniform 0;
    }
}


