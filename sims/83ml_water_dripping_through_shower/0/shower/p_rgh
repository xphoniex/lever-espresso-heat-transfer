FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/shower";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    defaultFaces
    {
        type            calculated;
        value           uniform 0;
    }
    wall
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
}


